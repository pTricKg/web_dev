var isEven = function(number) {
  // Your code goes here!
  if (number % 2 = 0) {
      return true;
  }else if (isNan(number)) {
      console.log("That is not a number!")
  }else {
      return false;
  }
};


//testing loop
var test = true;

while (test) {
    for (i = 0; i < 3; i ++){
        console.log("Testing.....Testing!!");
        test = false;
    }
}
